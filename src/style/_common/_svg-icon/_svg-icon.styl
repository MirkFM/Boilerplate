@import "__"

_svg-icon__all($_allIcons)

@font-face
	font-family: $_i__font-name;
	src: url('../fonts/'+$_i__font-name+'.eot');
	src: url('../fonts/'+$_i__font-name+'.eot?#iefix') format('eot'),
		url('../fonts/'+$_i__font-name+'.woff') format('woff'),
		url('../fonts/'+$_i__font-name+'.ttf') format('truetype')
	font-weight: normal
	font-style: normal


.icon
	display: inline-block
	vertical-align: top
	color: $_c__dark
	transition: color 100ms ease
	font-size: 22px

	&:hover
		color: $_c__green

	& + &
		margin-left: 16px
